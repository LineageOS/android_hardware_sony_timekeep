cc_binary {
    name: "timekeep",
    srcs: ["timekeep.c"],
    shared_libs: [
        "libcutils",
        "liblog",
    ],
    init_rc: ["vendor/etc/init/timekeep.rc"],
    owner: "sony",
    proprietary: true,
}

android_app {
    name: "TimeKeep",
    srcs: ["src/**/*.java"],
    certificate: "platform",
    platform_apis: true,
    privileged: true,
    required: ["timekeep"],
}
